//A basic C Program to calculate sum of two numbers.				
#include<stdio.h>
int main(){
	int number1, number2, sum;
	scanf("%d %d", &number1, &number2); //Scanning values given by user.
	sum=number1+number2; //Processing the sum of two numbers.
	printf("Sum of 2 numbers= %d", sum); //Printing output.
	return 0;
} 